{
  "name": "kiera-bot",
  "version": "10.0.0",
  "main": "app/index.js",
  "repository": "https://github.com/rileyio/kiera-bot.git",
  "author": "Emma (RileyIO)",
  "contributors": [],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "trash app",
    "dev:live": "tsc -w",
    "dev": "DEBUG='kiera-bot:*' ts-node-dev --clear --respawn --ignore-watch='plugins' src/start.ts",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "start": " DEBUG='kiera-bot:*' nodemon src/start.ts",
    "start:nodebug": "node ./app/start.js",
    "test": "run-s clean build test:n",
    "test:n": "ava --tap --serial | tnyan",
    "test:v": "ava --verbose --serial",
    "test:verbose": "run-s clean build test:v"
  },
  "ava": {
    "failFast": false,
    "files": [
      "app/**/*.spec.js"
    ],
    "sources": [
      "app/**/*.js"
    ]
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "agenda": "^4.3.0",
    "axios": "^0.27.2",
    "blizzard.js": "^4.2.0",
    "crypto-js": "^4.1.1",
    "discord-api-types": "^0.37.1",
    "discord.js": "^14.1.2",
    "dot-prop": "^6.0.1",
    "dotenv": "^16.0.1",
    "fast-glob": "^3.2.11",
    "form-data": "^4.0.0",
    "gitly": "^2.2.1",
    "handlebars": "^4.7.7",
    "import-fresh": "^3.3.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongodb": "^3.7.3",
    "qrcode": "^1.5.1",
    "random": "^3.0.6",
    "restify": "^8.6.1",
    "socket.io": "^4.5.1",
    "winston": "^3.8.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/debug": "^4.1.7",
    "@types/got": "^9.6.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.6.4",
    "@types/qrcode": "^1.4.2",
    "@types/random": "^2.1.1",
    "@types/restify": "^8.5.4",
    "@types/restify-errors": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "ava": "^4.3.1",
    "debug": "^4.3.4",
    "eslint": "^8.21.0",
    "mongo_dottype": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tap-nyan": "^1.1.0",
    "trash-cli": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "xregexp": "^5.1.1"
  }
}
