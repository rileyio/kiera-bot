{
  "name": "kiera-bot",
  "version": "11.0-beta-3",
  "exports": "./app/start.js",
  "repository": "https://github.com/rileyio/kiera-bot.git",
  "author": "Emma (RileyIO)",
  "contributors": [],
  "license": "MIT",
  "private": true,
  "type": "module",
  "imports": {
    "#/*": "./src/*.ts",
    "#api/*": "./src/api/*.ts",
    "#commands/*": "./src/commands/*.ts",
    "#db": "./src/common/db/index.ts",
    "#integrations/*": "./src/integrations/*.ts",
    "#middleware": "./src/middleware/index.ts",
    "#objects/*": "./src/common/objects/*.ts",
    "#router/*": "./src/router/*.ts",
    "#secrets": "./src/secrets.ts",
    "#utils": "./src/common/utils/index.ts"
  },
  "engines": {
    "node": ">=18.7.0"
  },
  "scripts": {
    "build": "tsc",
    "dev:live": "tsc -w",
    "dev": "DEBUG='kiera-bot:*' nodemon --exec node --ignore '**/cache/**' --loader ts-node/esm src/start.ts",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "start": " DEBUG='kiera-bot:*' nodemon --exec node --ignore '**/cache/**' --loader ts-node/esm src/start.ts",
    "start:nodebug": "node ./app/start.js",
    "test": "run-s clean build test:n",
    "test:n": "ava --tap --serial | tnyan",
    "test:v": "ava --verbose --serial",
    "test:verbose": "run-s clean build test:v",
    "kiera": "./scripts/manage"
  },
  "ava": {
    "failFast": false,
    "files": [
      "app/**/*.spec.js"
    ],
    "sources": [
      "app/**/*.js"
    ]
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "agenda": "^5.0.0",
    "axios": "^1.6.7",
    "crypto-js": "^4.1.1",
    "discord-api-types": "^0.37.35",
    "discord-oauth2": "^2.11.0",
    "discord.js": "^14.11.0",
    "dot-prop": "^6.0.1",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "fast-glob": "^3.3.0",
    "gitly": "^2.4.0",
    "handlebars": "^4.7.7",
    "is-docker": "^3.0.0",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongodb": "^5.6.0",
    "nodemon": "^3.0.3",
    "random": "^4.1.0",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2",
    "yaml": "^2.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bson": "^4.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/debug": "^4.1.7",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.14.2",
    "@types/random": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "debug": "^4.3.4",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "typescript": "^5.1.3",
    "xregexp": "^5.1.1"
  }
}
