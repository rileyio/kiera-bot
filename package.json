{
  "name": "kiera-bot",
  "version": "9.2.1",
  "main": "app/index.js",
  "repository": "https://github.com/rileyio/kiera-bot.git",
  "author": "Emma (RileyIO)",
  "contributors": [],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "trash app",
    "dev:live": "tsc -w",
    "dev": "DEBUG='kiera-bot:*' ts-node-dev --respawn src/start.ts",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "start": " DEBUG='kiera-bot:*' nodemon src/start.ts",
    "start:nodebug": "node ./app/start.js",
    "test": "run-s clean build test:n",
    "test:n": "ava --tap --serial | tnyan",
    "test:v": "ava --verbose --serial",
    "test:verbose": "run-s clean build test:v"
  },
  "ava": {
    "failFast": false,
    "files": [
      "app/**/*.spec.js"
    ],
    "sources": [
      "app/**/*.js"
    ]
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@hapi/joi": "^17.1.1",
    "agenda": "^3.0.0",
    "blizzard.js": "^4.2.0",
    "crypto-js": "^4.1.1",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.6.0",
    "dot-prop": "^5.2.0",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.11",
    "form-data": "^3.0.0",
    "got": "^10.7.0",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.7.3",
    "qrcode": "^1.4.4",
    "random": "^2.2.0",
    "restify": "^8.6.1",
    "restify-cors-middleware": "^1.1.1",
    "socket.io": "^4.4.1",
    "winston": "^3.6.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/agenda": "^2.0.6",
    "@types/crypto-js": "^3.1.44",
    "@types/debug": "^4.1.7",
    "@types/got": "^9.6.12",
    "@types/hapi__joi": "^16.0.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^3.5.4",
    "@types/node": "^13.11.0",
    "@types/qrcode": "^1.4.2",
    "@types/random": "^2.1.1",
    "@types/restify": "^8.5.4",
    "@types/restify-cors-middleware": "^1.0.2",
    "@types/restify-errors": "^4.3.4",
    "@types/xregexp": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "ava": "^3.6.0",
    "debug": "^4.3.4",
    "eslint": "^8.12.0",
    "mongo_dottype": "^1.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "tap-nyan": "^1.1.0",
    "trash-cli": "^4.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "xregexp": "^4.3.0"
  }
}
