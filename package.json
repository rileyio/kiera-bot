{
  "name": "kiera-bot",
  "version": "8.0.0-beta-3",
  "main": "app/index.js",
  "repository": "https://github.com/rileyio/kiera-bot.git",
  "author": "Emma (RileyIO)",
  "contributors": [],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "trash app",
    "dev": "tsc -w",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "start:nodebug": "node ./app/start.js",
    "start": "DEBUG='kiera-bot:*' node ./app/start.js",
    "start:term:debug": "DEBUG='kiera-bot:*' node ./app/start.js",
    "test": "run-s clean build test:n",
    "test:n": "ava --tap --serial | tnyan",
    "test:v": "ava --verbose --serial",
    "test:verbose": "run-s clean build test:v"
  },
  "_moduleAliases": {
    "@": "app"
  },
  "ava": {
    "failFast": false,
    "files": [
      "app/**/*.spec.js"
    ],
    "sources": [
      "app/**/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 180,
    "trailingComma": "none"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "agenda": "^3.0.0",
    "blizzard.js": "^4.1.0",
    "chastikey.js": "github:ChastiKey/chastikey.js",
    "crypto-js": "^4.0.0",
    "discord.js": "^13.1.0",
    "dot-prop": "^5.2.0",
    "dotenv": "^8.2.0",
    "fast-glob": "^3.2.2",
    "form-data": "^3.0.0",
    "got": "^10.7.0",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.5.5",
    "qrcode": "^1.4.4",
    "random": "^2.2.0",
    "restify": "^8.5.1",
    "restify-cors-middleware": "^1.1.1",
    "socket.io": "^2.4.1",
    "winston": "^3.2.1",
    "yaml": "^1.9.2"
  },
  "devDependencies": {
    "@types/agenda": "^2.0.6",
    "@types/crypto-js": "^3.1.44",
    "@types/debug": "^4.1.5",
    "@types/got": "^9.6.9",
    "@types/hapi__joi": "^16.0.12",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongodb": "^3.5.4",
    "@types/node": "^13.11.0",
    "@types/qrcode": "^1.3.4",
    "@types/random": "^2.1.1",
    "@types/restify": "^8.4.2",
    "@types/restify-cors-middleware": "^1.0.1",
    "@types/restify-errors": "^4.3.3",
    "@types/socket.io": "^2.1.4",
    "@types/xregexp": "^4.3.0",
    "ava": "^3.6.0",
    "debug": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.3",
    "tap-nyan": "^1.1.0",
    "trash-cli": "^4.0.0",
    "tslint": "^6.1.1",
    "typescript": "^4.2.4",
    "xregexp": "^4.3.0"
  }
}
